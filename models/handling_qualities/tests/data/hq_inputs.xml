<FASTOAD_model>
	<data>
		<TLAR>
			<v_approach units="m/s">0.78<!--top-level requirement: approach speed--></v_approach>
			<v_cruise units="m/s">0.78<!--top-level requirement: cruise speed--></v_cruise>
		</TLAR>
		<aerodynamics>
			<aircraft>
				<landing>
					<CL_max>2.52<!--maximum lift coefficient in landing conditions--></CL_max>
				</landing>
				<low_speed>
					<CL_alpha>9.23<!--derivative of lift coefficient with respect to angle of attack in low speed conditions--></CL_alpha>
					<CL0_clean>0.8<!--value of lift coefficient at angle of attack 0 in clean configuration at low speed--></CL0_clean>
				</low_speed>
				<takeoff>
					<CL_max>2.34<!--maximum lift coefficient in takeoff conditions--></CL_max>
				</takeoff>
			</aircraft>
			<flaps>
				<landing>
					<CL>0.08<!--increment of CL due to flaps for landing phase--></CL>
				</landing>
				<takeoff>
					<CL>0.06<!--increment of CL due to flaps for takeoff phase--></CL>
				</takeoff>
			</flaps>
			<horizontal_tail>
				<low_speed>
					<CL_alpha>1.23<!--derivative of lift coefficient with respect to angle of attack in low speed conditions--></CL_alpha>
				</low_speed>
			</horizontal_tail>
			<vertical_tail>
				<cruise>
					<CnBeta>0.0023<!--derivative of yawing moment against sideslip angle for vertical tail in cruise conditions--></CnBeta>
				</cruise>
			</vertical_tail>
		</aerodynamics>
		<geometry>
			<fuselage>
				<length units="m">37.507<!--total fuselage length--></length>
			</fuselage>
			<has_T_tail>0.0<!--0=horizontal tail is attached to fuselage / 1=horizontal tail is attached to top of vertical tail--></has_T_tail>
			<horizontal_tail>
				<MAC>
					<at25percent>
						<x>
							<from_wingMAC25 units="m">16.55<!--distance along X between 25% MAC of wing and 25% MAC of horizontal tail--></from_wingMAC25>
						</x>
					</at25percent>
				</MAC>
			</horizontal_tail>
			<propulsion>
				<engine>
					<count>2.0<!--number of engines--></count>
					<z>
						<from_aeroCenter units="m">0.3<!--z-position of engine with respect to aero center--></from_aeroCenter>
						<from_wingMAC25 units="m">0.4<!--z-position of engine with respect to wing 25% MAC--></from_wingMAC25>
					</z>
				</engine>
				<nacelle>
					<diameter units="m">0.8<!--nacelle diameter--></diameter>
					<y units="m">2.3<!--Y-position of nacelle center--></y>
				</nacelle>
			</propulsion>
			<wing>
				<MAC>
					<at25percent>
						<x units="m">16.457<!--X-position of the 25% of mean aerodynamic chord of wing w.r.t. aircraft nose (drives position of wing along fuselage)--></x>
					</at25percent>
					<length units="m">4.457<!--length of mean aerodynamic chord of wing--></length>
				</MAC>
				<area units="m**2">124.843<!--wing reference area--></area>
				<span units="m">12.3<!--wing span--></span>
			</wing>
			<vertical_tail>
				<MAC>
					<at25percent>
						<x>
							<from_wingMAC25 units="m">16.55<!--distance along X between 25% MAC of wing and 25% MAC of vertical tail--></from_wingMAC25>
						</x>
					</at25percent>
				</MAC>
			</vertical_tail>
		</geometry>
		<mission>
			<sizing>
				<landing>
					<elevator_angle units="deg">-25.0<!--elevator maximum angle during landing phase in sizing mission--></elevator_angle>
					<thrust_rate>0.5<!--thrust rate (between 0.0 and 1.0) during landing phase in sizing mission--></thrust_rate>
				</landing>
				<main_route>
					<cruise>
						<altitude units="ft">15000<!--altitude during cruise phase in sizing mission--></altitude>
					</cruise>
				</main_route>
				<takeoff>
					<elevator_angle units="deg">-25.0<!--elevator maximum angle during takeoff phase in sizing mission--></elevator_angle>
					<thrust_rate>1.0<!--thrust rate (between 0.0 and 1.0) during takeoff phase in sizing mission--></thrust_rate>
				</takeoff>
			</sizing>
		</mission>
		<propulsion>
			<MTO_thrust units="N">30000<!--maximum thrust of one engine at sea level--></MTO_thrust>
		</propulsion>
		<weight>
			<aircraft>
				<CG>
					<aft>
						<x units="m">12.0<!--most aft X-position of aircraft center of gravity--></x>
					</aft>
				</CG>
				<MTOW units="kg">75797.99<!--maximum takeoff weight--></MTOW>
				<MLW units="kg">35797.99<!--maximum landing weight--></MLW>
			</aircraft>
			<airframe>
				<landing_gear>
					<main>
						<CG>
							<x units="m">18.11<!--main landing gear (A51): X-position of center of gravity--></x>
						</CG>
					</main>
				</landing_gear>
			</airframe>
		</weight>
	</data>
	<settings>
		<weight>
			<aircraft>
				<CG>
					<range units="m">0.3<!--distance between front position and aft position of CG, as ratio of mean aerodynamic chord (allows to have front position of CG, as currently, FAST-OAD estimates only the aft position of CG)--></range>
				</CG>
			</aircraft>
		</weight>
	</settings>
</FASTOAD_model>
