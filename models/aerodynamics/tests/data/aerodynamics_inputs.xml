<FASTOAD_model>
	<data>
		<TLAR>
		  <v_cruise units="m/s">150<!--top-level requirement: cruise speed--></v_cruise>
		</TLAR>
		<geometry>
			<flap>
				<chord_ratio>0.197<!--mean value of (flap chord)/(section chord)--></chord_ratio>
				<span_ratio>0.8<!--ratio (width of flaps)/(total span)--></span_ratio>
			</flap>
			<flap_type>1.0<!--0=plain flap / 1=flap simple slotted--></flap_type>
			<fuselage>
				<front_length units="m">1.3378<!--length of front non-cylindrical part of the fuselage--></front_length>
				<length units="m">37.507<!--total fuselage length--></length>
				<maximum_height units="m">1.3378<!--maximum fuselage height--></maximum_height>
				<maximum_width units="m">1.1978<!--maximum fuselage width--></maximum_width>
				<rear_length units="m">1.3378<!--length of rear non-cylindrical part of the fuselage--></rear_length>
				<wetted_area units="m**2">401.956<!--wetted area of fuselage--></wetted_area>
			</fuselage>
			<has_T_tail>0.0<!--0=horizontal tail is attached to fuselage / 1=horizontal tail is attached to top of vertical tail--></has_T_tail>
			<horizontal_tail>
				<MAC>
					<at25percent>
						<x>
							<from_wingMAC25 units="m">6.5<!--distance along X between 25% MAC of wing and 25% MAC of horizontal tail--></from_wingMAC25>
							<local units="m">0.5<!--X-position of the 25% of mean aerodynamic chord of horizontal tail w.r.t. leading edge of root chord--></local>
						</x>
					</at25percent>
					<length units="m">0.5<!--mean aerodynamic chord length of horizontal tail--></length>
				</MAC>
				<area units="m**2">2.1<!--horizontal_tail area--></area>
				<root>
					<chord units="m">1.26<!--chord length at root of horizontal tail--></chord>
				</root>
				<span units="m">0.6<!--horizontal tail span--></span>
				<sweep_25 units="deg">28.0<!--sweep angle at 25% chord of horizontal tail--></sweep_25>
				<thickness_ratio>0.1<!--thickness ratio of horizontal tail--></thickness_ratio>
				<tip>
					<chord units="m">1.02<!--chord length at tip of horizontal tail--></chord>
				</tip>
				<wetted_area units="m**2">70.341<!--wetted area of horizontal tail--></wetted_area>
				<z>
					<from_wingMAC25 units="m">0.9<!--height of horizontal tail from 25% MAC of wing--></from_wingMAC25>
				</z>
			</horizontal_tail>
			<landing_gear>
				<height units="m">0.997<!--mean value of (flap chord)/(section chord)--></height>
			</landing_gear>
			<landing_gear_type>1.0<!--0=non-retractable landing gears / 1=retractable landing gear--></landing_gear_type>
			<propulsion>
				<engine>
					<count>2.0<!--number of engines--></count>
				</engine>
				<nacelle>
					<height units="m">2.211<!--nacelle height--></height>
					<length units="m">5.211<!--nacelle length--></length>
					<wetted_area units="m**2">21.609<!--wetted area of nacelle--></wetted_area>
					<width units="m">2.211<!--nacelle width--></width>
				</nacelle>
			</propulsion>
			<vertical_tail>
				<aspect_ratio>9.48<!--aspect ratio of vertical tail--></aspect_ratio>
				<root>
					<chord units="m">1.26<!--chord length at root of vertical tail--></chord>
				</root>
				<sweep_25 units="deg">35.0<!--sweep angle at 25% chord of vertical tail--></sweep_25>
				<thickness_ratio>0.1<!--thickness ratio of vertical tail--></thickness_ratio>
				<tip>
					<chord units="m">1.02<!--chord length at tip of vertical tail--></chord>
				</tip>
				<wetted_area units="m**2">52.749<!--wetted area of vertical tail--></wetted_area>
			</vertical_tail>
			<wing>
				<MAC>
					<at25percent>
						<x units="m">5.40327106<!--X-position of the 25% of mean aerodynamic chord of wing w.r.t. aircraft nose (drives position of wing along fuselage)--></x>
					</at25percent>
					<leading_edge>
						<x>
							<local units="m">1.2<!--X-position of leading edge of mean aerodynamic chord w.r.t. leading edge of root chord--></local>
						</x>
					</leading_edge>
					<length units="m">4.457<!--length of mean aerodynamic chord of wing--></length>
				</MAC>
				<area units="m**2">124.843<!--wing reference area--></area>
				<aspect_ratio>9.48<!--wing aspect ratio--></aspect_ratio>
				<kink>
					<span_ratio>0.11<!--ratio (Y-position of kink)/(semi-span)--></span_ratio>
				</kink>
				<root>
					<chord units="m">1.44095577151<!--chord length at wing root--></chord>
					<y units="m">0.2<!--Y-position of wing root--></y>
				</root>
				<span units="m">31.603<!--wing span--></span>
				<sweep_0 units="deg">0.0<!--sweep angle at leading edge of wing--></sweep_0>
				<sweep_25 units="deg">25.0<!--sweep angle at 25% chord of wing--></sweep_25>
				<taper_ratio>0.8<!--taper ratio of wing--></taper_ratio>
				<thickness_ratio>0.128<!--mean thickness ratio of wing--></thickness_ratio>
				<tip>
					<chord units="m">1.44095577151<!--chord length at wing tip--></chord>
					<y units="m">5.14330874945<!--Y-position of wing tip--></y>
				</tip>
				<wetted_area units="m**2">200.607<!--wetted area of wing--></wetted_area>
			</wing>
		</geometry>
		<mission>
		  <sizing>
			<landing>
			  <elevator_angle units="deg">-25.0<!--elevator maximum angle during landing phase in sizing mission--></elevator_angle>
			  <flap_angle units="deg">30.0<!--flap angle during landing phase in sizing mission--></flap_angle>
			</landing>
			<main_route>
				<cruise>
				  <altitude units="ft">35000.0<!--altitude during cruise phase in sizing mission--></altitude>
				</cruise>
			</main_route>
			<takeoff>
			  <elevator_angle units="deg">-25.0<!--elevator maximum angle during takeoff phase in sizing mission--></elevator_angle>
			  <flap_angle units="deg">10.0<!--flap angle during takeoff phase in sizing mission--></flap_angle>
			</takeoff>
		  </sizing>
		</mission>
		<weight>
			<aircraft>
				<MTOW units="kg">9000<!--maximum takeoff weight--></MTOW>
			</aircraft>
		</weight>
	</data>
</FASTOAD_model>
