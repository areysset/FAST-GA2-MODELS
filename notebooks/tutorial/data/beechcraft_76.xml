<FASTOAD_model>
  <data>
    <TLAR>
      <NPAX>2.0<!--top-level requirement: number of passengers, assuming a classic eco/business class repartition--></NPAX>
      <range units="NM">1000.0<!--top-level requirement: design range--></range>
      <v_approach units="kn">78.0</v_approach>
      <v_cruise units="kn">158.0</v_cruise>
      <v_limit units="kn">179.0</v_limit>
    </TLAR>
    <geometry>
      <flap_type>1.0</flap_type>
      <has_T_tail>1.0<!--0=horizontal tail is attached to fuselage / 1=horizontal tail is attached to top of vertical tail--></has_T_tail>
      <cabin>
        <aisle_width units="m">0.0<!--width of aisles--></aisle_width>
        <seats>
          <passenger>
            <count_by_row>2.0</count_by_row>
            <length units="m">0.8</length>
            <width units="m">0.565</width>
          </passenger>
          <pilot>
            <length units="m">0.75</length>
            <width units="m">0.565</width>
          </pilot>
        </seats>
      </cabin>
      <flap>
        <chord_ratio>0.197<!--mean value of (flap chord)/(section chord)--></chord_ratio>
        <span_ratio>0.8<!--ratio (width of flaps)/(total span)--></span_ratio>
      </flap>
      <horizontal_tail>
        <area units="m**2">4.422<!--horizontal tail area--></area>
        <sweep_25 units="deg">4.0<!--sweep angle at 25% chord of horizontal tail--></sweep_25>
        <taper_ratio>1.0<!--taper ratio of horizontal tail--></taper_ratio>
        <thickness_ratio>0.1<!--thickness ratio of horizontal tail--></thickness_ratio>
        <MAC>
          <at25percent>
            <x>
              <from_wingMAC25 units="m">4.334<!--distance along X between 25% MAC of wing and 25% MAC of horizontal tail--></from_wingMAC25>
            </x>
          </at25percent>
        </MAC>
      </horizontal_tail>
      <landing_gear>
        <type>1.0</type>
      </landing_gear>
      <propulsion>
        <count>2.0</count>
        <layout>1.0<!--position of engines (1=under the wing / 2=rear fuselage)--></layout>
        <y_ratio>0.34</y_ratio>
      </propulsion>
      <vertical_tail>
        <area units="m**2">2.507<!--vertical tail area--></area>
        <aspect_ratio>1.2<!--aspect ratio of vertical tail--></aspect_ratio>
        <sweep_25 units="deg">10.0<!--sweep angle at 25% chord of vertical tail--></sweep_25>
        <taper_ratio>0.62<!--taper ratio of vertical tail--></taper_ratio>
        <thickness_ratio>0.1<!--thickness ratio of vertical tail--></thickness_ratio>
      </vertical_tail>
      <wing>
        <area units="m**2">19.151<!--wing reference area--></area>
        <aspect_ratio>7.981<!--wing aspect ratio--></aspect_ratio>
        <sweep_25 units="deg">0.0<!--sweep angle at 25% chord of wing--></sweep_25>
        <taper_ratio>1.0<!--taper ratio of wing--></taper_ratio>
        <thickness_ratio>0.12<!--mean thickness ratio of wing--></thickness_ratio>
        <kink>
          <span_ratio>0.0<!--ratio (Y-position of kink)/(semi-span)--></span_ratio>
        </kink>
        <MAC>
          <at25percent>
            <x units="m">3.449<!--X-position of the 25% of mean aerodynamic chord of wing w.r.t. aircraft nose (drives position of wing along fuselage)--></x>
          </at25percent>
        </MAC>
      </wing>
    </geometry>
    <mission>
      <sizing>
        <fuel units="kg">0.0<!--consumed fuel mass during whole mission--></fuel>
        <cs23>
          <sizing_factor_ultimate>5.7</sizing_factor_ultimate>
        </cs23>
        <landing>
          <elevator_angle units="deg">-25.0</elevator_angle>
        </landing>
        <takeoff>
          <elevator_angle units="deg">-25.0</elevator_angle>
          <flap_angle units="deg">10.0<!--flap angle during takeoff phase in sizing mission--></flap_angle>
          <friction_coefficient_no_brake>0.03<!--ground friction coefficient with no brake in sizing mission--></friction_coefficient_no_brake>
          <thrust_rate>1.0</thrust_rate>
        </takeoff>
        <taxi_in>
          <duration units="s">300.0<!--duration of taxi-in phase in sizing mission--></duration>
          <speed units="kn">30.0<!--speed during taxi-in phase in sizing mission--></speed>
          <thrust_rate>0.3<!--thrust rate (between 0.0 and 1.0) during taxi-in phase in sizing mission--></thrust_rate>
        </taxi_in>
        <taxi_out>
          <duration units="s">300.0<!--duration of taxi-out phase in sizing mission--></duration>
          <speed units="kn">30.0<!--speed during taxi-out phase in sizing mission--></speed>
          <thrust_rate>0.3<!--thrust rate (between 0.0 and 1.0) during taxi-out phase in sizing mission--></thrust_rate>
        </taxi_out>
        <main_route>
          <climb>
            <thrust_rate>0.93</thrust_rate>
          </climb>
          <cruise>
            <altitude units="ft">8000.0<!--altitude during cruise phase in sizing mission--></altitude>
          </cruise>
          <descent>
            <descent_rate>0.05</descent_rate>
          </descent>
          <reserve>
            <duration units="min">30.0</duration>
          </reserve>
        </main_route>
      </sizing>
    </mission>
    <propulsion>
      <IC_engine>
        <fuel_type>1.0</fuel_type>
        <max_power units="W">130000.0</max_power>
        <strokes_nb>4.0</strokes_nb>
      </IC_engine>
    </propulsion>
  </data>
  <settings>
    <weight>
      <aircraft>
        <CG>
          <range>0.15<!--distance between front position and aft position of CG, as ratio of mean aerodynamic chord (allows to have front position of CG, as currently, FAST-OAD estimates only the aft position of CG)--></range>
        </CG>
      </aircraft>
    </weight>
  </settings>
</FASTOAD_model>
